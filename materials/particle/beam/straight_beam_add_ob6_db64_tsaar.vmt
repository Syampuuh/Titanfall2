"SpriteCard"
{
	$basetexture "particle\beam\straight_beam"
	$additive 	1

	$translucent 	1
	$nocull	1
	$splinetype	1

	$overbrightfactor 6

	$vertexFogAmount 1

	$depthblend 1
	$depthblendscale 64

	$disableTSAA 0
	$tsaaResponsive 1
	//$tsaaFullyResponsive 1
	$tsaaMotionAlphaThreshold 0.1
	$tsaaMotionAlphaRamp 1



}



