"Basic"
{
	"$basetexture" "vgui/hud/weapons/target_ring_back_pilot"
	"$translucent" "1"
	"$vertexcolor" "1"
	"$vertexalpha" "1"
	"$ignorez"     "1"
	
	"$basetexturerot" "0"
	"$time" "0.0"
	"$rate" "120.0"
	
	"Proxies"
	{
		"CurrentTime"
		{
			"resultVar" "$time"
		}
	
		"Multiply"
		{
			"srcvar1" "$time"
			"srcvar2" "$rate" 
			"resultvar" "$basetexturerot" 
		}
	
		"TextureTransform"
		{
			"rotateVar" "$basetexturerot"
			"resultvar" "$basetexturetransform"
		}
	} 
}